   "source": [
    "seq_raw = input(\"Please enter sequence into GC calculator: \")\n",
    "\n",
    "#print(\"The sequence you enter is: \" + seq_raw)\n",
    "\n",
    "seq = seq_raw.upper()\n",
    "\n",
    "g_count = seq.count(\"G\")\n",
    "c_count = seq.count(\"C\")\n",
    "\n",
    "gc_count = g_count + c_count\n",
    "\n",
    "seq_len = len(seq)\n",
    "\n",
    "gc_percent = gc_count / seq_len * 100\n",
    "\n",
    "gc_percent"
   ]
